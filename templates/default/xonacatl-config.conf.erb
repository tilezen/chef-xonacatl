listen <%= node[:xonacatl][:listen] %>
<% node[:xonacatl][:patterns].each do |pattern,origin| -%>
pattern <%= pattern %> -> <%= origin %>
<% end %>
<% node[:xonacatl][:headers].each do |header| -%>
header <%= header %>
<% end %>
<% if node[:xonacatl][:healthcheck] -%>
healthcheck <%= node[:xonacatl][:healthcheck] %>
<% end %>
